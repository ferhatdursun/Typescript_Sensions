{
    "compilerOptions": {
        "lib": ["ESNext"], 
        "target": "ES6", //! ES3 en düsük JS sürümüne göre düzenlemeler yapiyor. Detayli bilgi w3schools.com´da.
        "strict": true,
        "module": "CommonJS",
        "allowJs": false,
        "checkJs": false,
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "outDir": "dist",
        "rootDir": "src",
        "declaration": true,
    }
}